page {
    background-color: #f8f8f8;
}

.container {
    padding: 0 20rpx;
}

.order {
    margin: 15rpx 0;
    padding: 20rpx;
    border-radius: 10rpx;
    background-color: #ffffff;
}

.order-title {
    width: 100%;
    height: 80rpx;
    display: flex;
    justify-content: space-between;
}

.order-title .titleText {
    width: 65%;
    color: #333333;
    font-size: 34rpx;
    font-weight: bold;
    height: 80rpx;
    line-height: 80rpx;
    /*溢出隐藏*/
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.order .order-title .progress {
    height: 80rpx;
    font-size: 28rpx;
    font-weight: bold;
    line-height: 80rpx;
}

.order .order-title ._two {
    color: red;
}

.order .order-title ._one {
    color: #656565;
}

.order .order-title .zero {
    color: orange;
}

.order .order-title .one {
    color: dodgerblue;
}

.order .order-title .two {
    color: seagreen;
}
