page {
    background-color: #f6f6f6;
    display: flex;
    justify-content: center;
}

.container {
    width: 100%;
    display: flex;
    flex-direction: column;
    /* justify-content: center; */
}

.mine {
    display: flex;
    justify-content: center;
    width: 100%;
}

.mineCard {
    width: 85%;
    padding: 20rpx 40rpx;
    margin: 10rpx 0;
    border-radius: 30rpx;
    background-color: #ffffff;
    display: flex;
    flex-direction: column;
}

.head {
    font-size: 40rpx;
    text-align: center;
    font-weight: 600;
}

.name {
    margin-top: 20rpx;
    font-size: 30rpx;
}

.username {
    margin-top: 10rpx;
    font-size: 30rpx;
}

.about {
    margin-top: 10rpx;
}

.feedbackIcon {
    display: inline-block;
    line-height: 35rpx;
    float: left;
}

.aboutIcon {
    display: inline-block;
    line-height: 35rpx;
    float: left;
}

.feedbackIcon image {
    height: 35rpx;
    width: 35rpx;
}

.aboutIcon image {
    height: 35rpx;
    width: 35rpx;
}

.footer {
    display: inline-block;
    padding-left: 10rpx;
    line-height: 35rpx;
    font-size: 30rpx;
    float: left;
}